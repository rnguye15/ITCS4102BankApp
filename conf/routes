# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~

# An example controller showing a sample home page

GET     /                          controllers.loginController.login()
GET     /login                     controllers.loginController.login()
GET     /register                  controllers.loginController.register()
GET     /withdraw                  controllers.loginController.withdraw()
POST    /withdraw                  controllers.loginController.withdraw()
POST    /withdrawPost              controllers.loginController.withdrawPost()
POST    /depositPost               controllers.loginController.depositPost()
POST    /deposit                   controllers.loginController.deposit()
POST    /loginPost                 controllers.loginController.loginPost()
POST    /transfer                  controllers.loginController.transfer()
GET     /transfer                  controllers.loginController.transfer()
POST    /transferPost              controllers.loginController.transferPost()
POST    /registerPost              controllers.loginController.registerPost()
GET     /main                      controllers.loginController.mainMenu()
GET     /transaction               controllers.loginController.transaction()
GET     /back                      controllers.loginController.back()
GET     /account                   controllers.loginController.account()
GET     /changePass                controllers.loginController.changePass()
POST    /changePassPost            controllers.loginController.changePassPost()
GET     /logout                    controllers.loginController.logOut()
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)

